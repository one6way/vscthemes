    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "comment",
                "settings": {
                    "foreground": "#8a8a8a"
                }
            },
            {
                "scope": "keyword",
                "settings": {
                    "foreground": "#ff6b35"
                }
            },
            {
                "scope": "string",
                "settings": {
                    "foreground": "#ffa502"
                }
            },
            {
                "scope": "entity.name.function",
                "settings": {
                    "foreground": "#1e90ff"
                }
            },
            {
                "scope": "variable",
                "settings": {
                    "foreground": "#00cec9"
                }
            },
            {
                "scope": "support.function",
                "settings": {
                    "foreground": "#1e90ff"
                }
            },
            {
                "scope": "storage.type",
                "settings": {
                    "foreground": "#ff6b35"
                }
            },
            {
                "scope": "storage.modifier",
                "settings": {
                    "foreground": "#ff6b35"
                }
            },
            {
                "scope": "constant.numeric",
                "settings": {
                    "foreground": "#7bed9f"
                }
            },
            {
                "scope": "constant.language",
                "settings": {
                    "foreground": "#ff4757"
                }
            },
            {
                "scope": "entity.name.type",
                "settings": {
                    "foreground": "#48dbfb"
                }
            },
            {
                "scope": "entity.other.inherited-class",
                "settings": {
                    "foreground": "#48dbfb"
                }
            }
        ]
