  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6e7681",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#a5d6ff"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
